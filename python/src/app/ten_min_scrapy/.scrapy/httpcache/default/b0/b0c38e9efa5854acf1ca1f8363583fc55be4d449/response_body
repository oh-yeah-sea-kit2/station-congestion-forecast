var m_year = 2019;
var m_month = 10;
moveCalender();

$('.pl_calendarControl .pl_prev a').click(function(){
	m_year = m_month -1 > 0 ? m_year : m_year-1;
	if(m_year<1951){
		m_year = 1951;
		return false;
	}
	m_month = m_month -1 > 0 ? m_month -1 : m_month = 12;

	moveCalender();
	
	$('.pl_month').html(m_year+'/'+m_month);
	return false;
});

$('.pl_calendarControl .pl_next a').click(function(){
	m_year = m_month +1 <= 12 ? m_year : m_year+1;
	m_month = m_month +1 <= 12 ? m_month +1 : m_month = 1;

	moveCalender();
	
	$('.pl_month').html(m_year+'/'+m_month);
	return false;
});

function moveCalender(){
    type: 'POST',
	$.ajax({
	  url: "/_ajax/calendar.html",
	  data:{
		  year: m_year,
		  month: m_month
	  },
	  cache: true,
	  dataType: 'html',
	  success: function(html){
		$("table.pl_asideCalendar02").html(html);
	  }
	});
}
